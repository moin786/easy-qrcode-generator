{
    "name": "peal/laravel-qrcode-generator",
    "description": "QR code generate in Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed Minuddin(Peal)",
            "email": "moinuddin7@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "peal\\qrcodegenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "peal\\qrcodegenerator\\QrServiceProvider"
            ],
            "aliases": {
                "QR": "peal\\qrcodegenerator\\Facades\\QR"
            }
        }
    },
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "5.2.*|5.6.*|5.7.*|5.8.*"
    }
}
