{
    "name": "peal/qrcode-generator",
    "description": "QR code generate in Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Moinuddin Chowdhury",
            "email": "moinuddin7@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "peal\\qrcodegenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "peal\\qrcodegenerator\\QrServiceProvider"
            ],
            "aliases": {
                "Pdf": "peal\\qrcodegenerator\\Facades\\QR"
            }
        }
    },
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "5.6.*",
        "guzzlehttp/guzzle": "6.*"
    }
}
